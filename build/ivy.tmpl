<?xml version="1.0"?>
<ivy-module version="2.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
	<info
        organisation="com.junkbyte"
	    module="Console"
	    revision="@version@">

        <license
            name="Apache Licence 2.0"
            url="http://www.apache.org/licenses/LICENSE-2.0" />

        <repository
            name="as-artifacts"
            url="http://as-artifacts.org"
            pattern="http://as-artifacts.org/as3-repo-local/[organization]/[module]/[revision]/[artifact]-[revision].[ext]"
            ivys="true"
            artifacts="true"/>

        <description homepage="http://code.google.com/p/flash-console/">
            Flash-Console is Actionscript 3 logger/debugger for flash, flex and AIR. It comes with a collection of features to help speed up your development.
        </description>
    </info>

	<publications>
		<artifact type="swc" ext="swc" />
		<artifact type="src" ext="src.zip" />
	</publications>

	<!--
	<dependencies>
		<dependency org="{organisation}" name="{module}" rev="{revision}" >
			<artifact name="{module}" type="swc" ext="swc" />
			<artifact name="{module}" type="src" ext="src.zip" />
		</dependency>
	</dependencies>
	-->
</ivy-module>